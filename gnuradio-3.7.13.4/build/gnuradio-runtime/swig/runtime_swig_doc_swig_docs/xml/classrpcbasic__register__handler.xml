        <type></type>
        <definition>rpcbasic_register_handler&lt; T &gt;::rpcbasic_register_handler</definition>
        <argsstring>(const std::string &amp;block_alias, const char *handler, const char *units_=&quot;&quot;, const char *desc_=&quot;&quot;, priv_lvl_t minpriv_=RPC_PRIVLVL_MIN, DisplayType display_=DISPNULL)</argsstring>
